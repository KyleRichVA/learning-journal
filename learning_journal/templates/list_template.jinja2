{% extends "base.jinja2" %}
{% block titlebar %}
<div class="navbar-header">
    <a class="navbar-brand" href="/">Kyle's Journal</a>
</div>
<ul class="nav navbar-nav">
    <li><a href="{{ request.route_url("add_entry") }}">Add Entry</a></li>
</ul>
{% endblock titlebar %}
{% block content %}
<div class="posts container">
    <ul>
    {% for entry in entries %}
        <li><a href="{{ request.route_url("detail", id = entry.id) }}">{{ entry.title }}</a>-date</li>
    {% endfor %}
    </ul>
</div>
{% endblock content %}
