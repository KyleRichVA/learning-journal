{% extends "base.jinja2" %}
{% block content %}
<div class="posts container">
    <div class="list-group">
    {% for entry in entries %}
        <a href="{{ request.route_url("detail", id = entry.id) }}" class="list-group-item">
        <h4 class="list-group-item-heading">{{ entry.title }}</h4>
        <p class="list-group-item-text">{{ entry.created.date() }}</p>
        </a>
    {% endfor %}
    </div>
</div>
{% endblock content %}
